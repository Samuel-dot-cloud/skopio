  "query": "\n        SELECT\n          g.id,\n          g.name,\n          g.target_seconds,\n          g.time_span,\n          g.use_apps,\n          g.use_categories,\n          g.ignore_no_activity_days,\n          g.created_at,\n          g.updated_at,\n          ga.app,\n          gc.category,\n          gd.day\n        FROM goals g\n        LEFT JOIN goal_apps ga ON g.id = ga.goal_id\n        LEFT JOIN goal_categories gc ON g.id = gc.goal_id\n        LEFT JOIN goal_excluded_days gd ON g.id = gd.goal_id\n        ORDER BY g.id\n        ",
